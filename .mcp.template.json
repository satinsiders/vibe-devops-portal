{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "."],
      "env": {},
      "description": "File system operations - read, write, search files"
    },
    "github": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "YOUR_GITHUB_TOKEN_HERE"
      },
      "disabled": true,
      "description": "GitHub repository management - PRs, issues, files"
    },
    "supabase": {
      "command": "npx",
      "args": ["-y", "@supabase/mcp-server"],
      "env": {
        "SUPABASE_URL": "YOUR_SUPABASE_URL_HERE",
        "SUPABASE_SERVICE_KEY": "YOUR_SUPABASE_SERVICE_KEY_HERE"
      },
      "disabled": true,
      "description": "Supabase database operations, auth, and storage"
    },
    "memory": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-memory"],
      "env": {},
      "disabled": true,
      "description": "Persistent knowledge graph memory across sessions"
    },
    "slack": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-slack"],
      "env": {
        "SLACK_BOT_TOKEN": "YOUR_SLACK_BOT_TOKEN_HERE",
        "SLACK_TEAM_ID": "YOUR_SLACK_TEAM_ID_HERE"
      },
      "disabled": true,
      "description": "⭐ RECOMMENDED: Slack workspace integration for team communication"
    },
    "vercel": {
      "type": "http",
      "url": "https://mcp.vercel.com",
      "env": {
        "VERCEL_API_TOKEN": "YOUR_VERCEL_TOKEN_HERE"
      },
      "disabled": true,
      "description": "Vercel deployment management and logs"
    },
    "render": {
      "type": "http",
      "url": "https://mcp.render.com/mcp",
      "env": {
        "RENDER_API_KEY": "YOUR_RENDER_API_KEY_HERE"
      },
      "disabled": true,
      "description": "Render deployment and infrastructure management"
    },
    "firecrawl": {
      "command": "npx",
      "args": ["-y", "firecrawl-mcp"],
      "env": {
        "FIRECRAWL_API_KEY": "YOUR_FIRECRAWL_API_KEY_HERE"
      },
      "disabled": true,
      "description": "⭐ RECOMMENDED: Web scraping and content extraction"
    },
    "cloudflare-docs": {
      "type": "http",
      "url": "https://docs.mcp.cloudflare.com",
      "env": {},
      "disabled": true,
      "description": "Search Cloudflare documentation"
    },
    "cloudflare-workers-builds": {
      "type": "http",
      "url": "https://builds.mcp.cloudflare.com/mcp",
      "env": {
        "CLOUDFLARE_API_TOKEN": "YOUR_CLOUDFLARE_TOKEN_HERE"
      },
      "disabled": true,
      "description": "Cloudflare Workers build management"
    },
    "cloudflare-workers-bindings": {
      "type": "http",
      "url": "https://bindings.mcp.cloudflare.com/mcp",
      "env": {
        "CLOUDFLARE_API_TOKEN": "YOUR_CLOUDFLARE_TOKEN_HERE"
      },
      "disabled": true,
      "description": "Cloudflare Workers bindings (D1, R2, KV)"
    },
    "cloudflare-observability": {
      "type": "http",
      "url": "https://observability.mcp.cloudflare.com/mcp",
      "env": {
        "CLOUDFLARE_API_TOKEN": "YOUR_CLOUDFLARE_TOKEN_HERE"
      },
      "disabled": true,
      "description": "Cloudflare monitoring and analytics"
    },
    "context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"],
      "env": {},
      "disabled": true,
      "description": "⭐ RECOMMENDED: Up-to-date package documentation and code examples"
    },
    "magic-ui": {
      "command": "npx",
      "args": ["-y", "@magicuidesign/mcp"],
      "env": {},
      "disabled": true,
      "description": "Magic UI components library"
    },
    "playwright": {
      "command": "npx",
      "args": ["-y", "@playwright/mcp"],
      "env": {},
      "disabled": true,
      "description": "Browser automation for testing"
    }
  }
}
